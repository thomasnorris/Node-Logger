{
    "app_name": "Node-Logger",
    "debug_mode": true,
    "enable_uncaught_exception_binding": true,
    "sql": {
        "connection": {
            "user": "username",
            "password": "password",
            "server": "localhost\\sqlexpress",
            "database": "Database",
            "port": 1433,
            "options": {
                "enableArithAbort": true
            }
        },
        "sp": {
            "log_node_app": {
                "name": "SP_LogNodeApp",
                "params": {
                    "app_name": "appName",
                    "message": "message",
                    "log_type_ID": "logTypeID"
                }
            }
        }
    },
    "log_types": {
        "debug": 0,
        "info": 1,
        "warning": 2,
        "error": ,
        "uncaught_exception": 4
    },
    "messages": {
        "init": "App started.",
        "default": "Message not supplied.",
        "uncaught_exception_binding": {
            "enabled": "Uncaught exception binding enabled for this app.",
            "disabled": "Uncaught exception binding disabled for this app."
        },
        "debug_mode_enabled": "Logging debug mode enabled for this app."
    }
}